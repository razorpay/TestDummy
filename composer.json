{
    "name": "laracasts/testdummy",
    "description": "Easy test stubs",
    "keywords": ["testing", "laravel", "factorygirl", "stubs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jeffrey Way",
            "email": "jeffrey@laracasts.com"
        },
        {
            "name": "Team Razorpay",
            "email": "support@razorpay.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "11.44.1",
        "fakerphp/faker": "~1.4"
    },
    "require-dev": {
        "phpspec/phpspec": "7.5.x-dev",
        "illuminate/database": "11.44.1",
        "phpunit/phpunit": "^10.5.35"
    },
    "autoload": {
        "psr-4": {
            "Laracasts\\TestDummy\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/support/models"
        ]
    },
    "minimum-stability": "dev"
}
